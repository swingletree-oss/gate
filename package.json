{
  "name": "@swingletree-oss/gate",
  "description": "Gate",
  "repository": {
    "type": "git",
    "url": "git@github.com:swingletree-oss/gate.git"
  },
  "main": "main.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "npm run build && node bin/main.js",
    "build": "npm run build-ts && npm run tslint",
    "test": "ts-mocha --exit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "eslint",
    "debug": "npm run build && npm run watch-debug",
    "watch-debug": "nodemon --inspect bin/main.js",
    "release": "semantic-release"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "ts"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": 75,
    "timeout": 2000,
    "ui": "bdd",
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.spec.ts"
    ],
    "watch-ignore": [
      "lib/vendor"
    ],
    "recursive": true,
    "require": [
      "reflect-metadata",
      "./test/mocha.env.js"
    ]
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "@octokit/rest": "16.43.2",
    "@octokit/webhooks": "^7.9.3",
    "@swingletree-oss/harness": "^2.0.3",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "express-github-webhook": "^1.0.6",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "node-cache": "^5.1.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@semantic-release/exec": "^5.0.0",
    "@types/basic-auth": "^1.1.3",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.18",
    "@types/chai-things": "0.0.34",
    "@types/compression": "1.7.0",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.52",
    "@types/node-cache": "^4.2.5",
    "@types/redis": "^2.8.28",
    "@types/request": "^2.48.5",
    "@types/sinon": "^9.0.11",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^7.27.0",
    "mocha": "^8.4.0",
    "mockserver": "^3.1.1",
    "nodemon": "^2.0.7",
    "npm-check-updates": "^7.1.1",
    "semantic-release": "^17.4.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.7.0",
    "sinon-express-mock": "^2.2.1",
    "ts-mocha": "^7.0.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.9"
  },
  "license": "GPL-3.0",
  "private": true
}
